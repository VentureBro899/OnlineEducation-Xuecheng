<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xuecheng.user.mapper.OauthClientTokenMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.xuecheng.user.model.po.OauthClientToken">
        <id column="authentication_id" property="authenticationId" />
        <result column="token_id" property="tokenId" />
        <result column="token" property="token" />
        <result column="user_name" property="userName" />
        <result column="client_id" property="clientId" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        token_id, token, authentication_id, user_name, client_id
    </sql>

</mapper>
